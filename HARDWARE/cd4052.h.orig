    RTD1=0X0000, 	 //0000 0000 0000 0000
    RTD2=0X0040,   //0000 0000 0100 0000
    RTD3=0X0080,   //0000 0000 1000 0000
    RTD4=0X00C0,   //0000 0000 1100 0000
} RTD;


#define RTDOUT(RTD) GPIOB->ODR=(GPIOB->ODR&0xff0f)|(RTD&0x00f0);      //7-4‰Ωç

